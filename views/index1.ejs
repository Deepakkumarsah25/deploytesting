    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const semesterButtons = document.querySelectorAll('.semester-btn');
            const scheduleContainers = document.querySelectorAll('.schedule-container');
            
            semesterButtons.forEach(button => {
                button.addEventListener('click', function() {
                    const semesterId = this.getAttribute('data-semester');
                    
                    // Remove active class from all buttons and containers
                    semesterButtons.forEach(btn => btn.classList.remove('active'));
                    scheduleContainers.forEach(container => container.classList.remove('active'));
                    
                    // Add active class to clicked button and corresponding container
                    this.classList.add('active');
                    document.getElementById(`semester-${semesterId}`).classList.add('active');
                });
            });
        });
    </script>
